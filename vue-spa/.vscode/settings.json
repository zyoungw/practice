    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.idea": true,
    "**/.svn": true,
    "**/build": true,
    "**/CVS": true,
    "**/dist": true,
    "**/node_modules": true
  }
